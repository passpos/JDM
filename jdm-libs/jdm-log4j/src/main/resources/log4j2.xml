<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
    <Appenders>
        <!--
        <Appender type="" name="" fileName="">
            <Layout type="" pattern="">
                <Pattern></Pattern>
            </Layout>
        </Appender>-->
        <Console name="Console" target="SYSTEM_OUT">
            <PatternLayout pattern="%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"></PatternLayout>
        </Console>
    </Appenders>
    <Loggers>
        <Logger name="com.passpos" level="INFO"></Logger>
        <Root level="ERROR">
            <AppenderRef ref="Console"></AppenderRef>
        </Root>
    </Loggers>

</Configuration>
